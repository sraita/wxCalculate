.theme-tech.container{
  background-color: #313233;
  color: #37e0fc;
}
/* 状态提示栏 */
.theme-tech .calc-tip-box{
  color: #37e0fc;
}
/* 输入输出显示区域 */
.theme-tech .calc-input-box{
  color: #37e0fc;
}
/* 按钮统一设置 */
.theme-tech .btn{
  background-color: transparent !important;
  color: #37e0fc;
  background-image: url('data:image/png;base64,');
  background-size:100% 100%;
	background-repeat:no-repeat;
  background-position: center;
}
.theme-tech .btn.btn-hover {
  background-image: url('data:image/png;base64,');
}

/* 工具栏 */
.theme-tech .tool-list {
  position: relative;
  height: 56px;
  padding: 0;
  background-color: #272727;
  border-radius: 5px;
  box-shadow: 0 1px 5px 1px #00b0cf;
}

.theme-tech .tool-item{
  padding: 0; padding-bottom: 2px;
  background-color: #161617;
}
.theme-tech .tool-item .btn {
  top: -2px;
  background-image: linear-gradient(#353b3e,#222223);
  line-height: 56px;
}
.theme-tech .tool-item .iconfont {color: #37e0fc;}
.theme-tech .tool-item .btn.btn-hover{
  background-image: linear-gradient(#222223,#353b3e);
  box-shadow: inset 1px 1px 4px 0px rgba(0,0,0,.3);
}

/* 数字按钮 - 主体按钮区域 */
.theme-tech .btn-item .btn {
  left: 2px;
  right: 2px;
  top: 2px;
  bottom: 2px;
}

/* 历史记录页面 */
.theme-tech .his-item { 
  border-bottom: 1px solid #24444a;
}
/* 设置页面 */
.theme-tech .set-box{
  background-color: #353b3e;
}
.theme-tech .set-item {
  color: #37e0fc;
  border-color: #313131;
}
.theme-tech .theme-content { border-color: #313131; }
.theme-tech .theme-content rich-text {
  background-color: #3a3a3a;
  color: #37e0fc;
}